
<div class="mt-4">
    <div class="row">

        {{#each products}}
          <div class="col-sm-6 col-lg-4">
            <div class="card card-product-item">
                <img class="card-img-top" src="{{this.image}}" alt=""/>
    
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.description}}</p>
                    <a href="#!" class="btn btn-primary">Go Product</a>
                </div>
            </div>
          </div>

        {{/each}}
    </div>
</div>
